{
  "index": "/index.html",
  "routes": {
    "/": {
      "match": "exact"
    },
    "/menu": {
      "match": "exact"
    },
    "/ordenes": {
      "match": "prefix"
    },
    "/nueva-orden": {
      "match": "prefix"
    },
    "^/regex/.*/route$": {
      "match": "regex"
    }
  },
  "external": {
